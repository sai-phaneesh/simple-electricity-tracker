{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "electricity",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "electricity (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "electricity (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    },
    {
      "name": "Flutter for web (hot reloadable)",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "chrome",
        "--web-port",
        "8080",
        "--web-experimental-hot-reload"
      ]
    },
    {
      "name": "Flutter for web (Chrome)",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": ["-d", "chrome", "--web-port", "8080"]
    },
    {
      "name": "Flutter for web (Edge)",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": ["-d", "edge", "--web-port", "8080"]
    },
    {
      "name": "Flutter for web (server only)",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": ["-d", "web-server", "--web-port", "8080"]
    }
  ]
}
